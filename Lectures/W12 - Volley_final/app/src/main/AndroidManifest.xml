<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="acad277.http_get">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="acad277.http_get.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="acad277.http_get.VolleyGetActivity"></activity>
        <activity android:name="acad277.http_get.RetrofitGetActivity"></activity>
        <activity android:name="acad277.http_get.AsyncTaskGetActivity"></activity>
        <activity android:name="acad277.http_get.LanguagesActivity"></activity>
    </application>

</manifest>